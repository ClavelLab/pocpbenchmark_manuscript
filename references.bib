@article{kuzmanovicTaxonomyRhizobiaceaeRevisited2022,
  title = {Taxonomy of {{Rhizobiaceae}} Revisited: Proposal of a New Framework for Genus Delimitation},
  shorttitle = {Taxonomy of {{Rhizobiaceae}} Revisited},
  author = {Kuzmanović, Nemanja and Fagorzi, Camilla and Mengoni, Alessio and Lassalle, Florent and {diCenzo}, George C.},
  date = {2022},
  journaltitle = {International Journal of Systematic and Evolutionary Microbiology},
  volume = {72},
  number = {3},
  pages = {005243},
  publisher = {Microbiology Society,},
  issn = {1466-5034},
  doi = {10.1099/ijsem.0.005243},
  url = {https://www.microbiologyresearch.org/content/journal/ijsem/10.1099/ijsem.0.005243},
  urldate = {2025-01-07},
  abstract = {The alphaproteobacterial family \&\#160;\&\#160;Rhizobiaceae\&\#160;\&\#160; is highly diverse, with 168 species with validly published names classified into 17 genera with validly published names. Most named genera in this family are delineated based on genomic relatedness and phylogenetic relationships, but some historically named genera show inconsistent distribution and phylogenetic breadth. The most problematic is \&\#160;\&\#160;Rhizobium\&\#160;\&\#160;, which is notorious for being highly paraphyletic, as most newly described species in the family are assigned to this genus without consideration of their proximity to existing genera, or the need to create novel genera. Moreover, many \&\#160;\&\#160;Rhizobiaceae\&\#160;\&\#160; genera lack synapomorphic traits that would give them biological and ecological significance. We propose a common framework for genus delimitation within the family \&\#160;\&\#160;Rhizobiaceae\&\#160;\&\#160;, wherein genera are defined as monophyletic groups in a core-genome gene phylogeny, that are separated from related species using a pairwise core-proteome average amino acid identity (cpAAI) threshold of approximately 86{$\mkern1mu\%$}. We further propose that additional genomic or phenotypic evidence can justify division of species into separate genera even if they share greater than 86{$\mkern1mu\%$} cpAAI. Applying this framework, we propose to reclassify \&\#160;\&\#160;Rhizobium rhizosphaerae\&\#160;\&\#160; and \&\#160;\&\#160;Rhizobium oryzae\&\#160;\&\#160; into Xaviernesmea gen. nov. Data is also provided to support the formation of Peteryoungia aggregata comb. nov., Endobacterium yantingense comb. nov., Neorhizobium petrolearium comb. nov., Pararhizobium arenae comb. nov., Pseudorhizobium tarimense comb. nov. and Mycoplana azooxidifex comb. nov. Lastly, we present arguments that the unification of the genera \&\#160;\&\#160;Ensifer\&\#160;\&\#160; and \&\#160;\&\#160;Sinorhizobium\&\#160;\&\#160; in Opinion 84 of the Judicial Commission is no longer justified by current genomic and phenotypic data. Despite pairwise cpAAI values for all \&\#160;\&\#160;Ensifer\&\#160;\&\#160; species and all \&\#160;\&\#160;Sinorhizobium\&\#160;\&\#160; species being \&gt;86{$\mkern1mu\%$}, additional genomic and phenotypic data suggest that they significantly differ in their biology and ecology. We therefore propose emended descriptions of \&\#160;\&\#160;Ensifer\&\#160;\&\#160; and \&\#160;\&\#160;Sinorhizobium\&\#160;\&\#160;, which we argue should be considered as separate genera.},
  file = {/home/cpauvert/Zotero/storage/69CDNBSM/Kuzmanović et al. - 2022 - Taxonomy of Rhizobiaceae revisited proposal of a new framework for genus delimitation.pdf}
}

