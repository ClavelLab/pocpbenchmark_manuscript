@article{afrizalEnhancedCulturedDiversity2022,
  title = {Enhanced Cultured Diversity of the Mouse Gut Microbiota Enables Custom-Made Synthetic Communities},
  author = {Afrizal, Afrizal and Jennings, Susan A. V. and Hitch, Thomas C. A. and Riedel, Thomas and Basic, Marijana and Panyot, Atscharah and Treichel, Nicole and Hager, Fabian T. and Wong, Erin Oi-Yan and Wolter, Birger and Viehof, Alina and family=Strempel, given=Alexandra, prefix=von, useprefix=false and Eberl, Claudia and Buhl, Eva M. and Abt, Birte and Bleich, André and Tolba, René and Blank, Lars M. and Navarre, William W. and Kiessling, Fabian and Horz, Hans-Peter and Torow, Natalia and Cerovic, Vuk and Stecher, Bärbel and Strowig, Till and Overmann, Jörg and Clavel, Thomas},
